apiVersion: apps/v1
kind: Deployment
metadata:
  name: mynginx2
spec:
  replicas: 3
  selectors:
    matchLabels:
      app: nginx
      env: prod
    spec:
      containers:
        - name: nginx
          image: nginx
          resources:
            requests:
              cpu: 100m
              memory: 129Mi
            limits:
              cpu: 250m
              memory: 256Mi
            ports:
              - containerPort: 80
